{
  "name": "google-form-csv2json",
  "version": "0.1.0",
  "authors": [
    "plato@societypro.org"
  ],
  "description": "A node script to format google forms CSV data into json; and a html chart of some data",
  "private": false,
  "scripts": {
    "prestart": "npm install; npm run render-test-json;",
    "start": "node_modules/http-server/bin/http-server -p 8000",
    "render-test-json": "node js/node-makeJson.js data/brave-c3i-members.csv"
  },
  "dependencies": {
    "csv": "~0.3.7",
    "http-server": "^0.6.1"
  },
  "keywords": [
    "bravecollective",
    "google",
    "csv",
    "form",
    "d3js"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ]
}
